---
import Favicon from "../components/Favicon.astro";
const { title, description } = Astro.props;
---

<head>
    <meta charset="UTF-8" />
    <meta
        name="viewport"
        content="width=device-width, initial-scale=1, maximum-scale=1"
    />
    <meta
        name="description"
        content={description}
    />
    <meta
        name="generator"
        content={Astro.generator}
    />
    <Favicon />
    <title>{title}</title>
</head>
